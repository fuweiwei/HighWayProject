<?xml version="1.0" encoding="utf-8"?>
<com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center"
    android:orientation="vertical">
    <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_heightPercent="45%"
        android:background="@color/white"
        app:layout_widthPercent="70%">
        <com.ty.highway.frameworklibrary.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_heightPercent="10%">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/title_activity_tunneldetails"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/common_margin"
                android:textColor="@color/black"
                app:layout_textSizePercent="50%" />

        </com.ty.highway.frameworklibrary.support.percent.PercentRelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.0dp"
            android:background="@color/common_theme_color"/>
        <ScrollView
            android:id="@+id/activity_taskdetails_scr"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_heightPercent="75%"
            android:layout_margin="@dimen/common_margin"
            android:background="@color/main_bg_color"
            >
            <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/common_margin"
                    android:gravity="center"
                    >
                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_code"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_code"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00000"
                        android:singleLine="true"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/common_margin"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_name"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        android:singleLine="true"
                        android:text="00000"
                        />

                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_road"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_road"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        android:text="00000"
                        android:singleLine="true"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_start"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_start"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        android:text="00000"
                        android:singleLine="true"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_end"  android:textSize="16sp"
                        android:textColor="@color/black"

                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00000"
                        android:singleLine="true"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>




                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_length"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_length"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00000"
                        android:singleLine="true"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_width"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_width"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00000"
                        android:singleLine="true"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        android:text="@string/tunnel_heigth"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_heigth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        android:singleLine="true"
                        android:text="00000"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_completiondate"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        android:text="00000"
                        android:singleLine="true"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
                <com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/common_margin"
                    android:layout_marginBottom="@dimen/common_margin"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tunnel_remarks"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        />
                    <TextView
                        android:id="@+id/activity_tunneldetails_remarks"
                        android:layout_width="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/black"
                        android:layout_height="wrap_content"
                        android:text="00000"
                        android:singleLine="true"
                        />
                </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
            </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
        </ScrollView>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.0dp"
            android:background="@color/common_theme_color"/>
        <Button
            android:id="@+id/activity_tunneldetails_cancel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            app:layout_textSizePercent="5%"
            android:background="@drawable/common_click_btn_selector"
            android:textColor="@color/black"
            android:text="取消"/>
    </com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>
</com.ty.highway.frameworklibrary.support.percent.PercentLinearLayout>